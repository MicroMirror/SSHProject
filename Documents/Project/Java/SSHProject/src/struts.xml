<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<include file="struts-default.xml" />
	<package name="ynwi" extends="struts-default">
		<action name="register" class="regAction">
			<result name="input">/Register.jsp</result>
			<result name="success">/Success.jsp</result>
			<result name="error">/Fail.jsp</result>
		</action>
		<action name="testhtml" class="com.ynwi.ssh.action.TestAction">
		    <result name="testhtml">/testhtml.jsp</result>
		</action>
		<action name="printhtml" class="com.ynwi.ssh.action.PrintHTML">  
           <result name="printhtml">/printhtml.jsp</result>  
       </action> 
	</package>

</struts>
